

<h2>you have saved <%= @user.SearchResult.count%> articles </h2>
<div class="col-md-12">
    <table id="dtBasicExample" class="table table-striped table-bordered table-sm" cellspacing="0" width="100%">
      <thead>
        <tr>
          <th class="th-sm">Title
          </th>
          <th class="th-sm">Link
          </th>
          <th class="th-sm">date created
          </th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <h1><% @user.SearchResult.each do | result | %></h1>
          <td><%= result.title%></td>
          <td><%= result.link%></td>
          <td><%= result.created_at%> </td>
        </tr>
          <% end %>
      </tbody>
    </table>
</div>
